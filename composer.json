{
    "name": "desolatormagno/laravel-msg",
    "description": "Paquete para mostrar mensajes en el front",
    "keywords": ["laravel", "blade", "sweetalert2", "desolatormagno","laravel-msg"],
    "license": "MIT",
    "homepage": "https://github.com/DesolatorMagno/laravel-msg",  
    "authors": [
        {
            "name": "Reny Ramos",
            "email": "renyramosgarcia@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DesolatorMagno\\LaravelMsg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DesolatorMagno\\LaravelMsg\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DesolatorMagno\\LaravelMsg\\LaravelMsgServiceProvider"
            ]
        }
    }
}
